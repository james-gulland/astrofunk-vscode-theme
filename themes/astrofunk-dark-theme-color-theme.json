    "tokenColors": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#8991c1CC",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "meta.parameters comment.block",
        "settings": {
          "foreground": "#B9C2E0",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["variable.other.constant.object", "variable.other.readwrite.alias", "meta.import variable.other.readwrite"],
        "settings": {
          "foreground": "#FFCA85"
        }
      },
      {
        "scope": ["variable.other", "support.type.object"],
        "settings": {
          "foreground": "#ecf5ff",
        }
      },
      {
        "scope": ["variable.other.object.property", "variable.other.property", "support.variable.property"],
        "settings": {
          "foreground": "#ecf5ff"
        }
      },
      {
        "scope": [".method", "string.unquoted", "meta.object.member"],
        "settings": {
          "foreground": "#B9C2E0"
        }
      },
      {
        "scope": ["variable - meta.import", "variable.other.readwrite.alias", "constant.other.placeholder", "meta.object-literal.key-meta.object.member"],
        "settings": {
          "foreground": "#ecf5ff"
        }
      },
      {
        "scope": ["keyword.control.flow", "storage.modifier.async", "storage.modifier.await"],
        "settings": {
          "foreground": "#93E2D4",
           "fontStyle": "italic"
        }
      },
      {
        "scope": ["keyword.operator.new", "keyword.control.new", "keyword.control.default"],
        "settings": {
          "foreground": "#93E2D4",
        }
      },
      {
        "scope": [
          "variable.language.this", "storage.modifier", "variable.language.super"
        ],
        "settings": {
          "foreground": "#93E2D4",
        }
      },
      {
        "scope": [         
          "support.class.error", 
          "keyword.control.trycatch", 
